.basc_info .basc_left h2 > span { color: #9a0e0e; font-size: 14px; letter-spacing: 1px; }
.basc_info .vcrh2 { margin: 5px 10px 10px; }
.basc_info .vcrh2 .myvcr { height: 24px; background: url(images/basc_jlvcr.gif) no-repeat left center; padding-left: 24px; color: #538c29; display: inline-block; }
.basc_info .basc_ulinfo { float: left; }
.basc_info .basc_ulinfo > li { float: left; width: 250px; line-height: 30px; }
.basc_info .basc_ulinfo > li.allline { float: left; height: 30px; width: 500px; line-height: 30px; }
.basc_info .basc_right { float: left; width: 170px; padding: 10px; }
.basc_info .listul { float: left; padding: 10px; }
.basc_info .listul > li { float: left; width: 90%; line-height: 30px; }
.basc_info .listul2 { float: left; padding: 10px; }
.basc_info .listul2 > li { float: left; line-height: 30px; }

.basc_info p { padding: 10px; line-height: 22px; }
.basc_info p span { display: inline-block; word-break: break-all; }

.basc_info .pline { margin: 10px; border-style: dashed; color: #dedede; }



