/****tab选项卡样式****/
.offer_tab { margin: 15px; position: relative; }
.offer_tab .tab_title { position: absolute; float: left; z-index: 2; }
.offer_tab .tab_title > ul > li { float: left; margin-right: 10px; border: 1px solid #dedede; }
.offer_tab .tab_title > ul > li > a { display: inline-block; padding: 8px 30px; border-top: 4px solid #62b2fb; color: #FFF; font: bold 14px arial; letter-spacing: 2px; }
.offer_tab .tab_title > ul > li > a.btn { background: -moz-linear-gradient(top, #62b2fb, #489eeb); background: -o-linear-gradient(top, #62b2fb, #489eeb); background: -webkit-gradient(linear, 0 0, 0 bottom, from(#62b2fb), to(#489eeb)); filter: progid:DXImageTransform.Microsoft.gradient(startcolorstr=#62b2fb,endcolorstr=#489eeb,gradientType=0); background: -ms-linear-gradient(top, #62b2fb, #489eeb); background: linear-gradient(top, #62b2fb, #489eeb); }
.offer_tab .tab_title > ul > li.tab_selected { border-bottom: 1px solid #f9f9fb; }
.offer_tab .tab_title > ul > li.tab_selected > a { border-top-color: #62b2fb; background: #f9f9fb; color: #333; filter: none; }
.offer_tab .tab_content { margin-top: 37px; float: left; border: 1px solid #dedede; background: #f9f9fb url(images/search_qbg.gif) right bottom no-repeat; width: 100%; }
.offer_tab .tc_main { padding: 20px 0; float: left; position: relative; }
.offer_tab .tc_main > ul { float: left; }
.offer_tab .tc_main > ul > li { margin-top: 10px; float: left; position: relative; }
.offer_tab .tc_main .btndiv { float: right; margin-top: 10px; margin-right: 7%; }


/*弹框样式*/
.bpopup_wrap { overflow: hidden; position: relative; display: none; }

.bpopup_top { width: 100%; height: 33px; background: -moz-linear-gradient(top, #ffffff, #ececec); background: -o-linear-gradient(top, #ffffff, #ececec); background: -webkit-gradient(linear, 0 0, 0 bottom, from(#ffffff), to(#ececec)); filter: progid:DXImageTransform.Microsoft.gradient(startcolorstr='#ffffff', endColorstr='#ececec',gradientType=0); background: -ms-linear-gradient(top, #ffffff, #ececec); background: linear-gradient(top, #ffffff, #ececec); }

.bpopup_top span { height: 33px; float: left; display: block; }

.bpopup_top .leftshadow { width: 3px; background-position: 0 0; background-repeat: no-repeat; }

.bpopup_top .rightshadow { width: 9px; background-position: -4px 0; background-repeat: no-repeat; }

.bpopup_top .titlebg { background-position: 0 -35px; background-repeat: repeat-x; padding-left: 12px; font-size: 14px; color: #395c05; font-weight: bold; line-height: 36px; letter-spacing: 1.5px; }

.bpopup_main { padding: 0px; }

.bpopup_container { background-color: #fff; }

.bpopup_bottom { width: 100%; height: 10px; }

.bpopup_bottom span { height: 10px; float: left; display: block; }

.bpopup_bottom .leftshadow { width: 20px; background-repeat: no-repeat; }

.bpopup_bottom .rightshadow { width: 20px; background-repeat: no-repeat; }

.bpopup_bottom .shadowbg { width: 300px; }
/*提交信息弹框*/
.bpopup_wrap.submitinfo { width: 373px; /*大层的宽度*/ }

.bpopup_top .titlebg.submitinfo { width: 349px; /*比大层宽度少24px*/ }

.bpopup_main.submitinfo { width: auto; /*比大层宽度少8px*/ }

.bpopup_wrap.submitinfo .bpopup_container { height: 170px; }

.bpopup_bottom .shadowbg.submitinfo { width: 333px; /*比大层宽度少40px*/ }

/*弹框确定按钮*/
.bpopup_ok_btn { width: 109px; height: 29px; display: inline-block; border: none; cursor: pointer; background: url(../images/bpopup_btn.jpg) 0 0 no-repeat; }
/*弹框公共按钮*/
/*预定按钮*/
.bpopup_btn { width: 109px; height: 29px; display: inline-block; border: none; cursor: pointer; background: url(../images/bpopup_btnbg.png) 0 0 no-repeat; }
/*弹框取消按钮*/
.bpopup_cancel { width: 109px; height: 29px; display: inline-block; border: none; cursor: pointer; background: url(../images/bpopup_btn5.png) 0 0 no-repeat; }

/***简历信息***/
.basc_info { border: 1px solid #a9cdf1; width: 100%; float: left; margin-top: 10px; background: #FFF; }
.basc_info .basc_title { padding: 10px 20px; height: 20px; line-height: 20px; color: #1868b1; font-weight: bold; letter-spacing: 2px; font-size: 14px; border-bottom: 1px solid #a9cdf1; background: -moz-linear-gradient(top, #ffffff, #e2f0fe); background: -o-linear-gradient(top, #ffffff, #e2f0fe); background: -webkit-gradient(linear, 0 0, 0 bottom, from(#ffffff), to(#e2f0fe)); filter: progid:DXImageTransform.Microsoft.gradient(startcolorstr=#ffffff,endcolorstr=#e2f0fe,gradientType=0); background: -ms-linear-gradient(top, #ffffff, #e2f0fe); background: linear-gradient(top, #ffffff, #e2f0fe); }
.basc_info .basc_left { float: left; padding: 10px; width: 72%; }
.basc_info .basc_left h1 { border-bottom: 1px dashed #ccc; font-size: 26px; font-weight: bold; padding: 5px 0 15px 0; color: #618815; }
