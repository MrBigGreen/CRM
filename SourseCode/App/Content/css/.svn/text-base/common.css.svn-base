.pager .home_end, .pager .home_end a { width: 40px; }

.pager span { display: inline-block; /*width: 21px;*/ height: 20px; text-align: center; line-height: 20px; border: 1px solid #ddd; color: #616161; vertical-align: middle; }

.pager span.activeClass { border: none; padding: 0 3px; }

.pager a { display: inline-block; width: 21px; height: 20px; text-align: center; line-height: 20px; color: #616161; vertical-align: middle; /*border: 1px solid #ddd;*/ }

