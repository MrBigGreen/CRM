f@charset "utf-8";
/* CSS Document */

body { font-family: "宋体"; font-size: 12px;  color: #545454; margin: 0; padding: 0;  /*letter-spacing:0.5px;*/ }

table { font-size: 12px; border-collapse: collapse; border: none; }

table tr td, table tr th { border-collapse: collapse; word-break: break-all; padding: 0; }

h1, h2, h3, h4, h5, h6, p, ul, dl, dd { padding: 0; margin: 0; }

ul li { list-style: none; }

a { outline: none; text-decoration: none; }

a img { border: none; }

.wrap { min-width: 980px; }

.left { float: left; }

.right { float: right; }

.watermark { color: #a9a9a9!important; }
table { border-collapse: collapse; border: none; }

table tr td, table tr th { line-height:30px; border-collapse: collapse; word-break: break-all; }

input[type='button'], input[type='submit'] { outline: none; cursor: pointer; }

input[type='text'], input[type='password'] { border: 1px solid #dbdbdb; color: #000; padding-left: 3px; line-height: 20px; height: 18px; vertical-align: middle; margin: 0; }
textarea { border: 1px solid #dbdbdb; color: #000; padding-left: 3px; line-height: 22px; width: 690px; height: 110px; resize: none; }

input[type='checkbox'], input[type='radio'] { vertical-align: -3px; /**vertical-align:-1px;*/ }

select { vertical-align: middle; width: 125px; }

