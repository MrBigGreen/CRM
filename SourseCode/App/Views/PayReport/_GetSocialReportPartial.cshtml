<script src="~/Scripts/tableExport.js"></script>
<script src="~/Scripts/jquery.base64.js"></script>
<script type="text/javascript">

    function ExportExcel() {
        $('#tableInfo').tableExport({
            type: 'excel',
            separator: ';',
            escape: 'false',
            fileName: '社保支付审批表',
            worksheetName: 'sheet1'
        });
    }

</script>



