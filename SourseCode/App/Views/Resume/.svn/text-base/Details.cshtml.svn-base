            table += " </textarea></td></tr></tbody></table>";
            $("#divPrjExperience").append(table);
        }
        //*******************************************删除选中项目经验*******************************************//
        function getDelPrjExp() {
            $("#divPrjExperience").find("input[type='checkbox']:checked").each(function () {
                $(this).parent().parent().parent().parent().remove();
            });
        }


    </script>


