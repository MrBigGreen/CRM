                                        {
                                            sJobAddressName = string.Join("+", (from c in Model.TB_JobAddress select new { c.CityName }.CityName));
                                            sJobAddressCode = string.Join(",", (from c in Model.TB_JobAddress select new { c.CityCode }.CityCode));
