                <fieldset style="width: 100%">
                    <legend>
                        <label>原简历</label>
                    </legend>
                    <div id="divOldResume" class="divStyle" style=" overflow:auto; height:700px; width:100%;">
                        @{
                            string oldResume = "";
                            if (Model != null)
                            {
                                oldResume = Model.OldResume;
                            }
                        }
                        @Html.Raw(oldResume)

                    </div>
                </fieldset>
            </td>
        </tr>
    </table>
    <link href="~/Res/PublicCommon.css" rel="stylesheet" />
